{
  "name": "incap_backend",
  "version": "1.0.0",
  "description": "Back-end part of the Incap project",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "grunt": "grunt",
    "test": "nyc mocha --opts mocha.opts",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuanicom/incap.git"
  },
  "keywords": [
    "expressjs",
    "nodejs",
    "mean"
  ],
  "author": "Tuani SIMETE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tuanicom/incap/issues"
  },
  "homepage": "https://github.com/tuanicom/incap#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.7.3",
    "snyk": "^1.232.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/chai-http": "^4.2.0",
    "@types/chai-spies": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.19",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.22",
    "grunt-tslint": "^5.0.2",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "rewiremock": "^3.13.7",
    "source-map-support": "^0.5.13",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "server.ts",
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "snyk": true
}
